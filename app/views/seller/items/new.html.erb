<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new item</h1>
  <%= simple_form_for @item, url: seller_bundle_items_path(current_bundle) do |f| %>
    <br />
    <%= f.input :title %>
    <%= f.file_field :images, multiple: true %>

    <br />
    <br />
    <%= f.submit "Add an item" %>
  <% end %>
</div>
